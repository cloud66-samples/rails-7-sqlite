<h2>Hello World</h2>
<p>
  The time is now: <%= Time.now %>
</p>

<table>
  <caption>
    Existing Users
  </caption>
  <thead>
    <tr>
      <th scope="col">email</th>
      <th scope="col">UUID</th>
    </tr>
  </thead>
  <tbody>
    <% ::User.all.each do |user| %>
    <tr>
      <th scope="row"><%= user.email %></th>
      <td><%= user.uuid %></td>
    </tr>
    <% end %>
</table>

